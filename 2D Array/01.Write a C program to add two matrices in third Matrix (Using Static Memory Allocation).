//01.Write a C program to add two matrices in third Matrix (Using Static Memory Allocation).
#include<stdio.h>
void main()
{
    int i,j;
    int arr[3][3];
    int brr[3][3];
    int crr[3][3];
    
    
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
    {
        scanf("%d",&arr[i][j]);
    }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
    {
        printf("arr[%d][%d]=[%d]\t",i,j,arr[i][j]);
    }
        printf("\n");
    }   
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
    {
        scanf("%d",&brr[i][j]);
    }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
    {
        crr[i][j]=arr[i][j]+brr[i][j];
        printf("crr[%d][%d]=[%d]\t",i,j,crr[i][j]);
    }
        printf("\n");
    }
}

